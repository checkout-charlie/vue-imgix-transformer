{
  "name": "@sparwelt/vue-imgix-transformer",
  "version": "4.0.0",
  "description": "Vue component plugin to convert Imgix urls",
  "author": "Sparwelt",
  "license": "MIT",
  "keywords": [
    "imgix", "Vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sparwelt/vue-imgix-transformer.git"
  },
  "main": "dist/vue-imgix-transformer.common.js",
  "module": "dist/vue-imgix-transformer.esm.js",
  "unpkg": "dist/vue-imgix-transformer.min.js",
  "jsdelivr": "dist/vue-imgix-transformer.min.js",
  "scripts": {
    "start": "yarn build",
    "build": "rollup -c && minify dist/vue-imgix-transformer.js --out-file dist/vue-imgix-transformer.min.js --mangle.keepClassName"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-vue": "^4.3.0",
    "vue-template-compiler": "^2.5.16"
  }
}
